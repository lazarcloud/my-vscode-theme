  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#676f7d",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#f99157"
      }
    },
    {
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#7ec6cc"
      }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#ff8b94"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#c594c5"
      }
    },
    {
      "scope": ["keyword.operator", "constant.other.color.rgb-value"],
      "settings": {
        "foreground": "#7ec6cc"
      }
    },
    {
      "scope": ["function", "support.function", "entity.name.function"],
      "settings": {
        "foreground": "#6699cc"
      }
    },
    {
      "scope": [
        "type",
        "support.class",
        "entity.name.type",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#fac863"
      }
    },
    {
      "scope": ["string", "entity.name.tag"],
      "settings": {
        "foreground": "#99c794"
      }
    },
    {
      "scope": ["constant.numeric", "variable.language", "constant.language"],
      "settings": {
        "foreground": "#f99157"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#f99157"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#f99157"
      }
    },
    {
      "scope": ["support.constant"],
      "settings": {
        "foreground": "#7ec6cc"
      }
    },
    {
      "scope": ["support.type.property-name"],
      "settings": {
        "foreground": "#ff8b94"
      }
    },
    {
      "scope": ["meta.property-name", "entity.name.section"],
      "settings": {
        "foreground": "#6699cc"
      }
    },
    {
      "scope": [
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation"
      ],
      "settings": {
        "foreground": "#abb2bf"
      }
    }
  ]
}
